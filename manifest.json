{
  "manifest_version": 1,
  "id": "yo-notifier",
  "name": "Yo",
  "short_name": "Yo",
  "version": "0.1.4",
  "description": "Send a Yo to your phone",
  "homepage_url": "https://github.com/tim-hellhake/yo-notifier#readme",
  "license": "MPL-2.0",
  "author": "Tim Hellhake",
  "gateway_specific_settings": {
    "webthings": {
      "exec": "{nodeLoader} {path}",
      "strict_min_version": "0.10.0",
      "strict_max_version": "*",
      "primary_type": "notifier"
    }
  },
  "options": {
    "schema": {
      "type": "object",
      "required": [
        "apiToken"
      ],
      "properties": {
        "apiToken": {
          "type": "string",
          "title": "The api token"
        }
      }
    }
  }
}
